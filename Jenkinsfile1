@Library('my-shared-library') _
pipeline{
    agent any
    stages{
        stage('Git Checkout'){
            steps{
            gitCheckout(
                        branch: 'main',
                        url: 'https://github.com/SandeepPochu/mrdevops_java_app.git'
            )
            }
        }
        stage('maven unit test'){
            steps{
                script{
                    mvnTest()
                }
            }
        }
    }
}
